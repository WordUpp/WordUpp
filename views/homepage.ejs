<div class="row" class="center-align">
    <form class="center-align" class="col s10" action="/account/login" method="post">
      <div class="row">
        <div class="input-field col s10">
          <i class="material-icons prefix">account_circle</i>
          <input id="icon_prefix" type="text" name="username" value=""class="validate">
          <label for="icon_prefix">username</label>
        </div>
        <div class="input-field col s10">
          <i class="material-icons prefix">vpn_key</i>
          <input id="icon_password" type="password" name="password" value="" class="validate">
          <label for="icon_password">password</label>
        </div>
      </div>

        <button type="submit" class="submit" value="Login">GO</button>
    </form>
    <div class="center-align" id="registration">
        <a href="/account/registration" class="waves-effect waves-light btn-large">SIGN UP</a>
    </div>
</div>
